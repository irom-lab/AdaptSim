<?xml version="1.0"?>
<!-- from underactuated robotics - smaller dimensions -->
<sdf version="1.8">
  <model name='pendulum'>
    <link name="upper_arm">
      <inertial>
        <pose>0 0 -1 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0</iyy>
          <iyz>0</iyz>
          <izz>0</izz>
        </inertia>
      </inertial>
      <visual name="arm">
        <pose>0 0 -.5 0 0 0</pose>
        <geometry>
           <cylinder>
            <length>1</length>
            <radius>.01</radius>
           </cylinder>
        </geometry>
        <material><diffuse>0 0 0 1</diffuse></material>
      </visual>
      <visual name="point_mass">
        <pose>0 0 -1 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.1</radius>
          </sphere>
        </geometry>
        <material><diffuse>.6 .2 .2 1</diffuse></material>
      </visual>
    </link>

    <joint name="shoulder" type="revolute">
      <parent>world</parent>
      <child>upper_arm</child>
      <axis>
        <xyz>0 1 0</xyz>
        <!-- <dynamics>
          <damping>0.1</damping>
        </dynamics> -->
      </axis>
    </joint>
  </model>
</sdf>
